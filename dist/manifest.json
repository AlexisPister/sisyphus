{
  "manifest_version": 3,
  "name": "productivity",
  "descrition": "App for productivity",
  "version": "0.1",
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/block.ts-loader.js"
      ]
    }
  ],
  "action": {
    "default_title": "I dont know what im doing",
    "default_popup": "src/popup.html"
  },
  "web_accessible_resources": [
    {
      "use_dynamic_url": true,
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "**/*",
        "*"
      ]
    }
  ]
}